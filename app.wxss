/* app.wxss */

/**
 * Project: WooCommerce微信小程序
 * Description: 将WooCommerce商城接入微信小程序
 * Author: 
 * Organization: 
 */
@import "vendor/ZanUI/index.wxss";
@import "css/iconfont.wxss";



::-webkit-scrollbar {
    display: none;
    width: 0;
    height: 0;
    color: transparent;
}

page {
    --mainColor: #3db1b0;
    color: #444;
    font-size: 14px;
}

view {
    box-sizing: border-box;
    word-break: break-all;
}

text {
    line-height: 1.8;
}

button {
    color: #444;
    border-radius: 3px;
}

page button {
    border: 1px solid rgba(0, 0, 0, 0.2);
}

page button::after {
    display: none;
}

image, swiper, video {
    width: 100%;
    display: block;
}

.WxEmojiView {
    color: inherit;
}

.wxParse-Text {
    padding: 0 10px;
}

.primary-color {
    color: var(--mainColor);
}

.ellipsis-alt {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ellipsis {
    overflow: hidden;
    display: -webkit-box !important;
    word-break: break-all !important;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.ellipsis.ellipsis-2 {
    -webkit-line-clamp: 2;
}

.ellipsis.ellipsis-3 {
    -webkit-line-clamp: 3;
}

.thin-border {
    position: relative;
}

.thin-border::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 200%;
    transform-origin: 0 0;
    transform: scale(0.5, 0.5);
    box-sizing: border-box;
    z-index: 1;
}

.thin-border-top::before {
    border-top: 1px solid #ddd;
}

.thin-border-right::before {
    border-right: 1px solid #ddd;
}

.thin-border-bottom::before {
    border-bottom: 1px solid #ddd;
}

.thin-border-left::before {
    border-left: 1px solid #ddd;
}

.thin-border-all::before {
    border: 1px solid #ddd;
}

.loading text {
    font-size: 12px;
    margin-top: 0px !important;
}

.center {
    position: fixed;
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.center > view, .center > text {
    margin-top: -60px;
}

.center-title {
    background-color: #fff;
    font-size: 18px;
    text-align: center;
    padding: 12px 15px;
    position: relative;
}

.left-title-wrap {
    display: flex;
    justify-content: space-between;
    padding: 0 10px;
}

.left-title {
    font-size: 16px;
    text-align: left;
    padding-left: 15px;
    position: relative;
    line-height: 1.4;
}

.left-title::before {
    position: absolute;
    left: 0;
    top: 50%;
    content: "";
    display: block;
    width: 4px;
    height: 80%;
    border-radius: 4px;
    background-color: var(--mainColor);
    transform: translateY(-50%);
}

.left-title-wrap .title-view-more {
    height: 100%;
    display: flex;
    align-items: center;
}

.left-title-wrap .title-view-more image {
    width: 12px;
    height: 12px;
    margin-left: 5px;
}

.left-title-wrap .title-view-more text {
    color: #707070;
    font-size: 12px;
}

.arrow-up {
    width: 0;
    height: 0;
    border-left: 3px solid transparent;
    border-right: 3px solid transparent;
    border-bottom: 3px solid #aaa;
}

.arrow-down {
    width: 0;
    height: 0;
    border-left: 3px solid transparent;
    border-right: 3px solid transparent;
    border-top: 3px solid #aaa;
}

.btn {
    font-size: 14px;
    border: 0;
    transition: background-color 200ms ease-in-out;
}

.btn.disabled {
    background-color: #aaa;
}

.btn-primary {
    color: #fff;
    background-color: var(--mainColor);
}

.btn-main {
    color: var(--mainColor);
    border-color: var(--mainColor);
}

.single-line {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.product-price-wrapper {
    line-height: 1;
    padding: 5px 0;
    display: flex;
    align-items: flex-end;
    flex-wrap: wrap;
}

.product-price, .product-regular_price {
    display: flex;
    align-items: flex-end;
}

.product-price text, .product-regular_price text {
    line-height: 1;
    white-space: nowrap;
}

.product-price .currency {
    font-size: 13px;
}

.search-input-placeholder {
    color: rgba(61, 177, 176, 0.75);
    font-size: 14px;
}

.input-placeholder-gray {
    color: #707070;
    font-size: 14px;
}

.input-placeholder-thin-gray {
    color: #ddd;
    font-size: 14px;
}

.input-placeholder-black {
    color: #555;
    font-size: 14px;
}

.input-placeholder-small {
    font-size: 12px;
}

button .cart-quantity {
    display: block;
    font-size: 12px;
    min-width: 10px;
    height: 20px;
    line-height: 20px;
    color: #fff !important;
    background-color: var(--mainColor);
    border-radius: 10px;
    padding: 0 5px;
    white-space: nowrap;
}

.side-btn {
    position: fixed;
    right: 15px;
    display: flex;
    width: 40px;
    height: 40px;
    padding: 7px;
    border: 1px solid #ddd;
    border-radius: 50%;
    background-color: #fff;
    overflow: visible;
    z-index: 12;
}

.side-btn-card {
    border: 0;
    box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);
}

.side-btn image {
    width: 100%;
    height: auto;
}

.ui-nav-bar {
    box-shadow: 0 0px 5px 0px #dedede;
}

.navi-button {
    border-radius: 0;
    color: #555;
    background-color: #fff;
    border-color: #ddd;
    border-left: 0;
    border-right: 0;
    font-size: 14px;
    margin: 0;
    padding: 15px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.navi-button text {
    line-height: 1;
}

.navi-button image {
    width: 14px;
    height: 14px;
    margin-left: 5px;
}

/* ZanUI 加载更多 */

.zan-loadmore--nodata, .zan-loadmore--nomore,
.zan-loadmore.zan-loadmore--nomore .zan-loadmore__tips {
    background-color: inherit;
}

.zan-loadmore--nomore .zan-loadmore__tips {
    top: -11px;
    height: 19px;
    line-height: 19px;
}

.zan-loadmore__dot {
    width: 5px;
    height: 5px;
}

/* wxParse */

.wxParse-inline {
    line-height: 1.4;
}

/* 导航栏 */

.nav-bar-wrapper {
    width: 100%;
    padding: 0 10px;
    overflow: hidden;
}

.nav-bar-wrapper, .nav-bar-wrapper-wrap, .nav-bar-item {
    font-size: 16px;
    height: 100%;
    display: flex;
    align-items: center;
    overflow: hidden;
}

.nav-bar-item.title {
    color: #555;
}

.nav-bar-item.title text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.nav-bar-item.title.dark {
    color: #fff;
}

.nav-bar-item.back, .nav-bar-item.btns-placeholder {
    flex-shrink: 0;
}

.nav-bar-item.back {
    margin-left: -10px;
    padding: 7px;
}

.nav-bar-item.back:active {
    background-color: rgba(0, 0, 0, 0.2);
}

.nav-bar-item.back image {
    width: 25px;
    height: 25px;
}

.nav-bar-item.btns-placeholder {
    width: 95px;
}

/* 产品已选属性 */

.choose-wrapper {
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
    white-space: nowrap;
}

.choose-wrapper text {
    margin-right: 7px;
    color: #333;
}

/* 产品列表 */

.product-list {
    padding: 0 5px;
    display: flex;
}

.product-list .column {
    width: 50%;
}

.product-list .product-price .currency {
    font-size: inherit;
    vertical-align: initial;
}

.product-list .product {
    width: 100%;
    padding: 5px;
}

.product-list .product .price-row {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-shrink: 0;
}

.product-list .product .price-row .add-to-cart image {
    width: 18px;
    height: 18px;
}

.product .product-image-wrapper {
    position: relative;
}

.product .product-inner {
    border-radius: 8px;
    overflow: hidden;
    position: relative;
}

.product .product-main {
    background-color: #fff;
}

.product .product-detail {
    padding: 7px 5px;
}

.product .badge {
    width: 40px;
    height: 40px;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    color: #fff;
    position: absolute;
    top: 8px;
    left: 8px;
    font-size: 14px;
}

.product .badge.groupon {
    background-color: #fa4b48;
}

.product .badge.onsale {
    background-color: var(--mainColor);
}

.product .badge.outstock {
    background-color: #aaa;
}

.product .product-title, .product .product-price {
    display: block;
}

.product .product-title {
    font-size: 12px;
    word-break: break-word;
    line-height: 1.3;
}

.product .product-desc {
    font-size: 12px;
    color: #999;
}

.product .cart button {
    position: relative;
}

.product .cart button::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 200%;
    border-top: 1px solid #efefef;
    transform-origin: 0 0;
    transform: scale(0.5, 0.5);
    box-sizing: border-box;
    z-index: 1;
}

.product .add-to-cart {
    display: block;
    color: #333;
    border: 0;
    border-radius: 0;
    background-color: #fff;
}

.product .add-to-cart.button-hover {
    background-color: #f8f8f8;
}

.product .product-regular_price, .product .product-price,
.product-detail .product-regular_price, .product-detail .product-price {
    display: inline-block;
    font-size: 16px;
    overflow: hidden;
    padding: 1px 0;
}

.product .product-regular_price, .product-detail .product-regular_price {
    line-height: 1;
    color: #909090;
    text-decoration: line-through;
    font-size: 12px;
}

.product .product-price, .product-detail .product-price {
    line-height: 1;
    color: var(--mainColor);
    margin-right: 7px;
}

/* 产品列表-扁平 */

.product-list.product-list-flat {
    padding: 2px;
    flex-wrap: wrap;
    background-color: #fff;
}

.product-list.product-list-flat .product {
    width: 50%;
    padding: 2px;
    margin: 0;
}

.product-list.product-list-flat .product .product-inner {
    border-radius: 0;
}

/* 产品列表-滑动 */

.product-list.product-list-slide {
    padding: 0;
    background-color: #fff;
    padding-bottom: 45%;
    position: relative;
}

.product-list.product-list-slide swiper {
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.product-list.product-list-slide .product {
    padding: 2px;
    margin: 0;
}

.product-list.product-list-slide .product .product-inner {
    border-radius: 0;
}

.product-list.product-list-slide .product .product-title {
    -webkit-line-clamp: 1;
}

.product-list.product-list-slide .product .product-price-wrapper {
    padding-bottom: 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    display: inline-block;
}

.product-list.product-list-slide .product .add-to-cart {
    display: none;
}

.product-list.product-list-slide .product .points-icon {
    width: 10px;
    height: 10px;
}

.product-list.product-list-slide .product .product-price,
.product-list.product-list-slide .product .points-text-wrapper,
.product-list.product-list-slide .product .points-text {
    font-size: 12px;
}

.product-list.product-list-slide .product .badge {
    width: 30px;
    height: 30px;
    font-size: 10px;
}

/* W2W Extension, Name: w2w-products-favor, Code: selectStyle */

.product .select-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.3);
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
}

.product .checkbox {
    display: flex;
    align-items: center;
    position: absolute;
    top: 10px;
    right: 10px;
    border-radius: 50%;
    border: 1px solid #eee;
    background-color: rgba(255, 255, 255, 0.6);
    overflow: hidden;
}

.product .not-selected {
    width: 20px;
    height: 20px;
}

.product .icon {
    width: 25px;
    height: 25px;
}

/* W2W Extension, Name: w2w-products-favor, Code: selectStyle */

/* 选项卡 */

.zan-tab {
    height: auto;
}

.zan-tab .zan-tab__title {
    height: 48px;
    line-height: 48px;
}

.zan-tab .zan-tab__item--selected .zan-tab__title {
    color: var(--mainColor);
    border-bottom: 3px solid var(--mainColor);
}

.tab-content-wrapper {
    background-color: #fff;
}

.tab-content-wrapper .tab.hidden {
    display: none;
}

/* 弹窗按钮 */

.popup-btn {
    display: flex;
    width: 100%;
    height: 47px;
    font-size: 14px;
    text-align: left;
    border: 0;
    border-radius: 0;
    background-color: #fff;
}

.popup-btn .button-main {
    display: flex;
    align-items: center;
    flex-grow: 1;
    overflow: hidden;
}

.popup-btn .button-main .button-main-text {
    flex-shrink: 0;
    white-space: nowrap;
}

.popup-btn .go-popup {
    flex-shrink: 0;
    display: flex;
    align-items: center;
}

.popup-btn .go-popup image {
    width: 15px;
    height: 15px;
}

/* 弹窗 */

.login-popup .zan-popup__mask {
    z-index: 30;
}

.login-popup .zan-popup__container {
    z-index: 31;
}

.zan-popup .zan-popup__container {
    width: 100%;
    overflow: visible;
}

.zan-popup .close-btn {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 5px;
    right: 5px;
}

.zan-popup .popup-wrapper {
    width: 100%;
    padding: 0 15px;
    padding-bottom: 10px;
    overflow: visible;
}

.zan-popup .popup-wrapper .popup-title {
    font-size: 16px;
    padding: 15px 0;
}

.zan-popup .btn-primary.confirm {
    height: 50px;
    line-height: 50px;
    border-radius: 0;
}

/* 选择收货地址及订单地址 */

.block, .cart-list {
    margin: 10px 0;
}

.select-address.selected {
    height: auto;
    padding-top: 10px;
    padding-bottom: 10px;
}

.select-address.selected .button-main {
    display: block;
}

.select-address .customer-info, .select-address .address {
    line-height: 1;
}

.select-address .customer-info text, .select-address .address text {
    margin-right: 5px;
}

.select-address .address {
    color: #999;
    font-size: 12px;
}

/* 登录框 */

.login-popup .zan-popup__container {
    background-color: transparent;
}

.login-popup-wrapper {
    margin: 0 25px;
}

.login-popup button {
    background-color: transparent;
    padding: 0;
    border: 0;
}

.login-popup .login-inner {
    background-color: #fff;
    padding: 10px 15px;
    border-radius: 4px;
}

.login-inner {
    display: flex;
}

.login-inner .avatar {
    width: 50px;
    height: 50px;
    margin-right: 10px;
    flex-shrink: 0;
    border-radius: 50%;
}

.login-inner .close-btn {
    width: 20px;
    height: 20px;
}

.login-inner .username {
    font-size: 14px;
    display: flex;
    align-items: center;
}

.login-inner .username text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* 顶部提示 */

.zan-toptips {
    font-size: 12px;
    padding: 5px 10px;
    text-align: left;
}

.zan-toptips text {
    display: block;
    padding: 2px 0;
    line-height: 1.3;
}

/* 购物车数量 */

text.cart-quantity {
    position: absolute;
    top: -10px;
    right: 3px;
    height: 20px;
    min-width: 20px;
    line-height: 20px;
    font-size: 12px;
    border-radius: 10px;
    color: #fff;
    text-align: center;
    white-space: nowrap;
    transform: translateX(50%);
    transform-origin: center;
    z-index: 10;
    box-shadow: 0 0 0 1px #fff;
    box-sizing: border-box;
}

/* 产品评价 */

.review-list {
    background-color: #fff;
    padding: 0 15px;
}

.review-list .review {
    overflow: hidden;
    position: relative;
}

.review-list .review:before {
    content: "";
    position: absolute;
    top: -0.5px;
    left: 0;
    width: 200%;
    height: 200%;
    border-bottom: 1px solid #ddd;
    transform-origin: 0 0;
    transform: scale(0.5, 0.5);
    box-sizing: border-box;
    z-index: 1;
}

.review-header, .review-content, .review-images, .review-footer,
.review-children {
    margin: 10px 0;
}

.review-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.review-header-main {
    display: flex;
    align-items: center;
}

.review-header-avatar {
    width: 35px;
    height: 35px;
    margin-right: 8px;
}

.review-header-name {
    font-size: 14px;
}

.review-header-avatar image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.review-header-rate {
    display: flex;
    align-items: center;
}

.review-header-rate text {
    font-size: 12px;
    color: var(--mainColor);
    margin-left: 5px;
}

.review-header-rate image {
    width: 22px;
    height: 22px;
}

.review-content {
    font-size: 14px;
    margin-bottom: 10px;
}

.review-images {
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 1px;
    margin-bottom: 0;
}

.review-images .image {
    border: 1px solid #ddd;
    width: 80px;
    height: 80px;
    margin-bottom: 10px;
    margin-right: 10px;
    position: relative;
    z-index: 2;
}

.review-images .image.big {
    width: 160px;
    height: 160px;
}

.review-images .image image {
    width: 100%;
    height: 100%;
}

.review-footer {
    margin-top: 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.review-date {
    font-size: 12px;
    color: #999;
}

.review-reply {
    position: relative;
    z-index: 2;
}

.review-reply image {
    width: 20px;
}

.review-children {
    background-color: #f7f7f7;
    padding: 5px 10px;
}

.review-children .load-more {
    margin-top: 5px;
    padding: 8px 10px;
    display: flex;
    justify-content: center;
    align-items: center;
    border-top: 1px solid #eaeaea;
    position: relative;
    z-index: 2;
}

.child-review {
    padding: 5px 0;
    display: flex;
}

.child-review-avatar {
    width: 20px;
    height: 20px;
    border-radius: 50%;
    margin-top: 1px;
    margin-right: 7px;
    flex-shrink: 0;
}

.child-review-main {
    font-size: 12px;
}

.child-review-name.official {
    color: var(--mainColor);
}

.child-review-content {
    line-height: 1.4;
}

.reply-review-popup .zan-popup__container {
    width: 100%;
    padding: 13px 8px;
}

.reply-review-popup .reply-to {
    color: #888;
    font-size: 14px;
    margin-bottom: 10px;
}

.reply-review-popup .reply-to .reply-to-name {
    color: #444;
    font-weight: bold;
}

.reply-input-wrapper {
    display: flex;
}

.reply-input-wrapper .reply-input {
    flex-grow: 1;
}

.reply-input-wrapper .reply-input input {
    width: 100%;
    display: block;
    font-size: 14px;
    height: 40px;
    padding: 0px 10px;
    border: 1px solid #ddd;
    box-sizing: border-box;
}

.reply-input-wrapper .reply-send {
    display: flex;
    align-items: center;
    margin-left: 5px;
}

.reply-input-wrapper .reply-send button {
    border-radius: 0;
    border: 0;
    background-color: #fff;
    padding: 0;
}

.reply-input-wrapper .reply-send image {
    flex-shrink: 0;
    width: 30px;
    height: 30px;
}

/* 产品分享 */

.share-popup .zan-popup__container {
    display: flex;
    align-items: center;
    padding: 10px 0;
}

.share-popup .share-btn {
    width: 50%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.share-popup .share-btn button {
    width: 100%;
    height: 100%;
    padding: 0;
    background-color: #fff;
    border: 0;
    border-radius: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.share-popup .share-btn image {
    width: 35px;
    height: 35px;
    margin-bottom: 5px;
}

.share-popup .share-btn text {
    font-size: 12px;
}

/* 分享海报 */

.poster-popup .zan-popup__container {
    width: 80%;
    margin-top: 10px;
    background-color: transparent;
}

.poster-popup .poster-wrapper {
    width: 100%;
    height: 100%;
    display: flex;
